#!/bin/bash

ELISP=$( cat $HOME/.emacs_anywhere/emacs_anywhere.el )

if [ -z "$EA_EDITOR" ]; then
    EA_EDITOR="emacsclient -a '' -c"
fi

$EA_EDITOR -e "(progn
                 (defconst ea-app-name \"$EA_APP_NAME\")
                 (defconst ea-window-title \"$EA_WINDOW_TITLE\")
                 $ELISP)"
